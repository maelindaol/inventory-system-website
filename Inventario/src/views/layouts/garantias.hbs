<div class="conceptos">
    <div class="info">
        <h3>Garant√≠as de mobiliarios y equipos de computo registradas</h3>
        
        <form action="/garantias-mobiliario-equipo-computo/" method="GET">
            <div class="buscar">
                <input type="text" name="buscar" class="entradabuscar" placeholder="Buscar..." id="entrada">
                <button type="submit" class="btnbuscar" id="boton" value="Buscar">
                    <img src="../img/buscar.png" alt="">
                </button>
            </div> 
        </form>

        {{#if conceptos}}
            <table class="table" width="50%" heigth="70px">
            <thead>
                <tr>
                    <th>No. de resguardo</th>
                    <th>No. de interno</th>
                    <th>Nombre</th>
                    <th>Proveedor</th>
                    <th>Fecha de inicio</th>
                    <th>Fecha de vencimiento</th>
                </tr>
            </thead>
            <tbody>
                {{#each conceptos}}
                <tr>
                    <td>{{noresguardo}}</td>
                    <td>{{nointerno}}</td>
                    <td>{{nombre}}</td>
                    <td>{{garantia.proveedor}}</td>                    
                    <td>{{fecha}}</td>
                    <td>{{garantia.fechavencimiento}}</td>
                </tr>
                {{/each}}
            </tbody>
            </table>
        {{else}}
            <h4>No se encontraron registros...</h4>
        {{/if}}
    </div>
</div>