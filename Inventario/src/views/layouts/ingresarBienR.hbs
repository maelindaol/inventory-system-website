<div class="ingresarBien">
    <div class="info">
        <h3>Refacciones y accesorios a ingresar</h3>
            <form action="/ingresar-tipo-refacciones-accesorios" method="POST">
                <div class="ingresar">
                    <button type="submit" class="btningresar">Ingresar</button>
                    <input type="text" name="bienr" class="entrada" placeholder="Nombre" required="true">                
                </div>
            </form>
        {{#if bienrs}}
        <div class="tabla">
            <table class="table" width="50%" heigth="70px">
            <thead>
                <tr>
                    <th>Nombres:</th>
                    <th>Eliminar:</th>
                </tr>
            </thead>
            <tbody>
                {{#each bienrs}}
                <tr id="resultado">
                    <td>{{bienr}}</td>
                    <td>
                        <a href="/ingresar-tipo-refacciones-accesorios/delete/{{_id}}">
                            <button>
                                <img src="../img/delete_40.png" alt="" title="Eliminar">
                                Eliminar 
                            </button>
                        </a>
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
        {{else}}
            <h4>No se encontraron registros...</h4>
        {{/if}}
        </div>
    </div>
</div>